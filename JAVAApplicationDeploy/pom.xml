  <groupId>Java_App1</groupId>
  <artifactId>Java_App1</artifactId >
  <version>1.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <properties>
  <serverURL>${env.adminurl}</serverURL>
  <user>${env.weblogicuname}</user>
  <password>${env.weblogicpwd}</password>
  <maven.compiler.source>1.8</maven.compiler.source>
  <maven.compiler.target>1.8</maven.compiler.target>
  </properties>
  <build>
    <resources>
      <resource>
        <directory>/</directory>
        <includes>
          <include>**</include>
        </includes>
      </resource>
    </resources>	 
  </build>
  <modules>
    <module>Model</module>
    <module>ViewController</module>
  </modules>
